{
    "name": "axy/nginx-config-builder",
    "type": "library",
    "description": "Builds nginx config file",
    "keywords": [],
    "homepage": "https://github.com/axypro/nginx-config-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "axy/nginx-config-syntax": "~0.2.0"
    },
    "require-dev": {
        "axy/pkg-unit": "~0.1.1",
        "squizlabs/php_codesniffer": "=3.7.1"
    },
    "autoload": {
        "psr-4": {
            "axy\\nginx\\config\\builder\\": "src"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "axy\\nginx\\config\\builder\\tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            "/tests/*",
            "/devops/*",
            "/doc/*",
            "/*.sh",
            "/*.xml.dist",
            "/CHANGELOG.md"
        ]
    }
}
